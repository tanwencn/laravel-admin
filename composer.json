{
    "name": "tanwencn/laravel-admin",
    "description": "Laravel Admin",
    "type": "library",
    "keywords": ["laravel", "admin"],
    "homepage": "https://github.com/tanwencn/laravel-admin",
    "license": "MIT",
    "authors": [
        {
            "name": "tanwen",
            "email": "361657055@qq.com"
        }
    ],
    "require": {
        "laravel/framework" : "^5.5",
        "studio-42/elfinder": "~2.1.10",
        "barryvdh/elfinder-flysystem-driver": "^0.2.1",
        "league/flysystem": "^1.0",
        "league/flysystem-cached-adapter": "^1.0",
        "symfony/dom-crawler": "^4.1",
        "arrilot/laravel-widgets": "^3.12",
        "spatie/laravel-permission" : "^2.12"
    },
    "autoload": {
        "psr-4": {
            "Tanwencn\\Admin\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tanwencn\\Admin\\AdminServiceProvider"
            ],
            "aliases": {
                "Admin": "Tanwencn\\Admin\\Facades\\Admin"
            }
        }
    }
}
